{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch `gwh init`",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/gwh/",
            "cwd": "${workspaceFolder}",
            "args": [
                "init"
            ]
        },
        {
            "name": "Launch `gwh link repo1 .`",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/gwh/",
            "cwd": "${workspaceFolder}",
            "args": [
                "link",
                "repo1",
                "."
            ]
        },
        {
            "name": "Launch `gwh sync repo1`",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/gwh/",
            "cwd": "${workspaceFolder}",
            "args": [
                "sync",
                "repo1"
            ]
        }
    ]
}